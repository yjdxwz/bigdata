<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.it18zhang.eshop.model.UserItem" table="useritems">
        <id name="id" column="id" type="integer">
            <generator class="identity" />
        </id>
        <property name="userId" column="userid" type="int" />
        <property name="itemId" column="itemid" type="int" />
        <property name="score" column="score" type="int" />
        <property name="time" column="time" type="timestamp" />
    </class>
</hibernate-mapping>